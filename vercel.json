{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    },
    { "src": "/product", "dest": "/routes/product.route.js" },
    { "src": "/users", "dest": "/routes/users.route.js" }
  ]
}
